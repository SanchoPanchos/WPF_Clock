<Window x:Class="WPF_HW1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_HW1"
        xmlns:userControl="clr-namespace:WPF_HW1.UserControls"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Background="#cdc"
        Title="MainWindow" Height="400" Width="300">
    <Grid>
        <userControl:RegisterUserControl x:Name="registerUserControl"/>
        <userControl:LoginUserControl x:Name="loginUserControl"/>
        <userControl:MainUserControl x:Name="mainUserControl"/>
    </Grid>
</Window>
